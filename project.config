[receive]
	requireContributorAgreement = true
	requireChangeId = true
[submit]
	action = cherry pick
	mergeContent = true
[access "refs/*"]
	owner = group Administrators
	push = +force group Approvers
	forgeAuthor = group Approvers
	forgeCommitter = group Approvers
	create = group Approvers
[project]
	description = ssh://codereview.qt-project.org:29418/qt-labs/qtwebengine.git
[cherryPick]
	includeReviewedOn = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
[access]
	inheritFrom = All-Non-CI-Projects
