{
    "version": "1",
    "module-split": {
        "qtpdf": [
            "**/bin/*Pdf*",
            "**/include/*QtPdf*/**/*",
            "**/lib/cmake/Qt*Gui/*Pdf*",
            "**/lib/cmake/Qt*Pdf*/*",
            "**/lib/cmake/Qt*Qml/QmlPlugins/*Pdf*",
            "**/lib/metatypes/*pdf*",
            "**/lib/pkgconfig/*Pdf*",
            "**/lib/*Pdf*",
            "**/lib/static_chrome/*",
            "**/lib/QtPdf*.framework/**",
            "**/mkspecs/modules/qt_lib_pdf*",
            "**/mkspecs/modules/qt_plugin_qpdf.pri",
            "**/modules/Pdf*",
            "**/plugins/imageformats/**/*",
            "**/qml/QtQuick/**/*"
        ]
    }
}
